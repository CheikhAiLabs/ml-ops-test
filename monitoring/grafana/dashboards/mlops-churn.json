{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Total Predictions",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "predict_total",
          "legendFormat": "total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 100 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Churn Predictions",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "predict_churn_total",
          "legendFormat": "churn",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "thresholds": {
            "steps": [
              { "color": "orange", "value": null },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Churn Rate (%)",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "predict_churn_total / predict_total * 100",
          "legendFormat": "churn %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 30 },
              { "color": "red", "value": 60 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Prediction Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "rate(predict_total[1m])",
          "legendFormat": "requests/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "pointSize": 5
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Prediction Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(predict_latency_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(predict_latency_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(predict_latency_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Churn Rate Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "rate(predict_churn_total[5m]) / rate(predict_total[5m]) * 100",
          "legendFormat": "churn %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "fixed", "fixedColor": "orange" },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 30
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "API Health",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 14 },
      "targets": [
        {
          "expr": "up{job=\"inference-api\"}",
          "legendFormat": "API Status",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ],
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["mlops", "churn", "monitoring"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "MLOps - Churn Prediction Monitoring",
  "uid": "mlops-churn",
  "version": 1
}
