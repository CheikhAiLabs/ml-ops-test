{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "API Status",
      "type": "stat",
      "gridPos": { "h": 4, "w": 3, "x": 0, "y": 1 },
      "targets": [
        { "expr": "up{job=\"inference-api\"}", "legendFormat": "", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ],
          "thresholds": { "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        },
        "overrides": []
      }
    },
    {
      "title": "Uptime",
      "type": "stat",
      "gridPos": { "h": 4, "w": 3, "x": 3, "y": 1 },
      "targets": [
        { "expr": "time() - app_start_time_seconds", "legendFormat": "", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "fixed", "fixedColor": "blue" },
          "thresholds": { "steps": [{ "color": "blue", "value": null }] }
        },
        "overrides": []
      }
    },
    {
      "title": "Total Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 3, "x": 6, "y": 1 },
      "targets": [
        { "expr": "predict_total", "legendFormat": "total", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "blue" },
          "thresholds": { "steps": [{ "color": "blue", "value": null }, { "color": "green", "value": 100 }] }
        },
        "overrides": []
      }
    },
    {
      "title": "Churn Predicted",
      "type": "stat",
      "gridPos": { "h": 4, "w": 3, "x": 9, "y": 1 },
      "targets": [
        { "expr": "predict_churn_total", "legendFormat": "churn", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "thresholds": { "steps": [{ "color": "orange", "value": null }, { "color": "red", "value": 50 }] }
        },
        "overrides": []
      }
    },
    {
      "title": "No Churn",
      "type": "stat",
      "gridPos": { "h": 4, "w": 3, "x": 12, "y": 1 },
      "targets": [
        { "expr": "predict_no_churn_total", "legendFormat": "no churn", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "green" },
          "thresholds": { "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": []
      }
    },
    {
      "title": "Errors",
      "type": "stat",
      "gridPos": { "h": 4, "w": 3, "x": 15, "y": 1 },
      "targets": [
        { "expr": "predict_errors_total", "legendFormat": "errors", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "red" },
          "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] }
        },
        "overrides": []
      }
    },
    {
      "title": "Churn Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 3, "x": 18, "y": 1 },
      "targets": [
        { "expr": "predict_churn_total / predict_total", "legendFormat": "", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.3 }, { "color": "red", "value": 0.6 }] }
        },
        "overrides": []
      }
    },
    {
      "title": "Model Version",
      "type": "stat",
      "gridPos": { "h": 4, "w": 3, "x": 21, "y": 1 },
      "targets": [
        { "expr": "model_info", "legendFormat": "{{version}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "fixed", "fixedColor": "purple" },
          "thresholds": { "steps": [{ "color": "purple", "value": null }] },
          "mappings": [{ "type": "value", "options": { "1": { "text": "Loaded" } } }]
        },
        "overrides": []
      }
    },

    {
      "title": "Traffic & Throughput",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 6 },
      "targets": [
        { "expr": "rate(predict_total[1m])", "legendFormat": "predictions/s", "refId": "A" },
        { "expr": "sum(rate(http_requests_total[1m]))", "legendFormat": "all HTTP/s", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 20, "gradientMode": "scheme", "pointSize": 4, "showPoints": "auto" }
        },
        "overrides": []
      }
    },
    {
      "title": "HTTP Status Codes",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 6 },
      "targets": [
        { "expr": "sum by(status) (rate(http_requests_total[1m]))", "legendFormat": "{{status}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "bars", "lineWidth": 1, "fillOpacity": 80, "stacking": { "mode": "normal" } }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "200" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "400" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "500" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "503" }, "properties": [{ "id": "color", "value": { "fixedColor": "dark-red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "title": "Error Rate (%)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 6 },
      "targets": [
        { "expr": "rate(predict_errors_total[5m]) / rate(predict_total[5m]) * 100", "legendFormat": "error %", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "fixed", "fixedColor": "red" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 30 },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 5 }] }
        },
        "overrides": []
      }
    },

    {
      "title": "Latency",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Model Inference Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "targets": [
        { "expr": "histogram_quantile(0.50, rate(predict_latency_seconds_bucket[5m]))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, rate(predict_latency_seconds_bucket[5m]))", "legendFormat": "p95", "refId": "B" },
        { "expr": "histogram_quantile(0.99, rate(predict_latency_seconds_bucket[5m]))", "legendFormat": "p99", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10, "pointSize": 3, "showPoints": "auto" }
        },
        "overrides": []
      }
    },
    {
      "title": "HTTP Latency by Endpoint (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum by(le, endpoint) (rate(http_request_duration_seconds_bucket[5m])))", "legendFormat": "{{endpoint}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 15 }
        },
        "overrides": []
      }
    },

    {
      "title": "Predictions & Model Drift",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Churn Rate Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "targets": [
        { "expr": "rate(predict_churn_total[5m]) / rate(predict_total[5m]) * 100", "legendFormat": "churn %", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "fixed", "fixedColor": "orange" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 30 },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 30 }, { "color": "red", "value": 60 }] }
        },
        "overrides": []
      }
    },
    {
      "title": "Avg Churn Probability",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "targets": [
        { "expr": "rate(predict_probability_sum[5m]) / rate(predict_probability_count[5m])", "legendFormat": "avg P(churn)", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0, "max": 1,
          "color": { "mode": "fixed", "fixedColor": "yellow" },
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 25 }
        },
        "overrides": []
      }
    },
    {
      "title": "Predictions (Churn vs No Churn)",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "targets": [
        { "expr": "predict_churn_total", "legendFormat": "Churn", "refId": "A" },
        { "expr": "predict_no_churn_total", "legendFormat": "No Churn", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          { "matcher": { "id": "byName", "options": "Churn" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "No Churn" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      },
      "options": {
        "pieType": "donut",
        "legend": { "displayMode": "list", "placement": "right" }
      }
    },

    {
      "title": "Input Feature Distribution (Data Drift)",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Age Distribution",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 6, "x": 0, "y": 33 },
      "targets": [
        { "expr": "increase(feature_age_bucket[30m])", "legendFormat": "le {{le}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "lineWidth": 1, "fillOpacity": 70, "stacking": { "mode": "normal" } }
        },
        "overrides": []
      }
    },
    {
      "title": "Tenure Distribution",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 6, "x": 6, "y": 33 },
      "targets": [
        { "expr": "increase(feature_tenure_months_bucket[30m])", "legendFormat": "le {{le}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "lineWidth": 1, "fillOpacity": 70, "stacking": { "mode": "normal" } }
        },
        "overrides": []
      }
    },
    {
      "title": "Monthly Charges Distribution",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 6, "x": 12, "y": 33 },
      "targets": [
        { "expr": "increase(feature_monthly_charges_bucket[30m])", "legendFormat": "le {{le}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "lineWidth": 1, "fillOpacity": 70, "stacking": { "mode": "normal" } }
        },
        "overrides": []
      }
    },
    {
      "title": "Contract Type Split",
      "type": "piechart",
      "gridPos": { "h": 7, "w": 6, "x": 18, "y": 33 },
      "targets": [
        { "expr": "feature_contract_type_total", "legendFormat": "Type {{contract_type}}", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "pieType": "donut",
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["mlops", "churn", "monitoring", "production"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "ChurnGuard â€” Production Monitoring",
  "uid": "mlops-churn",
  "version": 2
}
